{% extends 'base.html.twig' %}

{% block title %}CityList{% endblock %}

{% block body %}
    <h1>CityList</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ city_list.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ city_list.name }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ city_list.url }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ city_list.createdAt ? city_list.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ city_list.updatedAt ? city_list.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('city_list_index') }}">back to list</a>

    <a href="{{ path('city_list_edit', {'id': city_list.id}) }}">edit</a>

    {{ include('city_list/_delete_form.html.twig') }}
{% endblock %}
